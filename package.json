{
  "name": "medical-records-server",
  "version": "1.0.0",
  "description": "RestAPI for Medical Records",
  "author": "Juan Manuel Alberro",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": "6.x",
    "npm": "3.9.x"
  },
  "scripts": {
    "start": "gulp serve",
    "build": "gulp",
    "lint": "gulp lint",
    "test": "gulp mocha",
    "commit": "git-cz",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "bark": "bithound check git@github.com:juan-manuel-alberro/medical-records-server.git"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:KunalKapadia/express-mongoose-es6-rest-api.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API"
  ],
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-validation": "^1.0.0",
    "express-winston": "^1.4.2",
    "http-status": "^0.2.3",
    "joi": "^9.0.0",
    "lodash": "^4.13.1",
    "method-override": "^2.3.6",
    "mongoose": "^4.5.3",
    "morgan": "^1.7.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "bithound": "^1.3.0",
    "chai": "^3.5.0",
    "commitizen": "^2.8.2",
    "coveralls": "^2.11.9",
    "cz-conventional-changelog": "^1.1.6",
    "del": "^2.2.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "ghooks": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-istanbul": "^1.0.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-mocha": "^2.2.0",
    "gulp-newer": "^1.2.0",
    "gulp-nodemon": "^2.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "isparta": "^4.0.0",
    "mocha": "^2.5.3",
    "run-sequence": "^1.2.2",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.2.0",
    "validate-commit-msg": "^2.6.1"
  },
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "npm test",
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
